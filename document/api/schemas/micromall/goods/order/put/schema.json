{
  "$schema": "http://json-schema.org/draft-03/schema",
  "type": "object",
  "properties": {
    "accesstoken": {
      "type": "string",
      "required": true,
      "description": "用户token（放在querystring中）"
    },
    "order": {
      "type": "array",
      "required": true,
      "description": "货架商品Id和货架商品的顺序 e.g: '货架商品Id': '货架商品的顺序'"
    }
  }
}